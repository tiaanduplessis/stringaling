    "string-utils"
  ],
  "homepage": "https://github.com/tiaanduplessis/stringaling#readme",
  "bugs": {
    "url": "https://github.com/tiaanduplessis/stringaling/issues"
  },
  "repository": {
    "url": "tiaanduplessis/stringaling",
    "type": "git"
  },
  "author": "tiaanduplessis",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  }
}
